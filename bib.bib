@inproceedings{visual_xenomai,
abstract = {<p>The visual navigation system for a mobile patrol robot using image processing by FPGA and real-time Linux is presented. The CMOS image sensor and the stepper motors driver ICs are connected to external I/O ports of the FPGA. The image processing and motor drive circuits are implemented into the reconfigurable device as original logic. The image capture circuit applies state machine and FIFO memory buffer to adjust timing for pixel data transmission. The motor drive circuit generates clock signals for steps according to the value from processor in the FPGA. The real- time device driver has been developed for the linkage between flexible hardware circuits and real-time software applications for robot vision purpose.</p>},
author = {Asami, Kenichi and Hagiwara, Hayato and Komori, Mochimitsu},
booktitle = {2011IEEE 10th International Conference on Trust, Security and Privacy in Computing and Communications},
isbn = {9781457721359},
keywords = {Real Time Systems ; Image Processing ; Field Programmable Gate Arrays ; Service Robots ; Robot Vision Systems ; Linux ; Cameras ; Autonomous Mobile Robot ; Patrol Service Robot ; Real-Time Image Processing ; Visual Navigation System},
