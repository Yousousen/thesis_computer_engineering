@article{delgado,
abstract = {<p>•A promising real-time architecture integrating robot software packages to Xenomai.•Deterministic response of Xenomai and rapid development using ROS.•Convenient APIs of communication mechanism between real-time/non-real-time tasks.•Flexible and extendible for real-time tasks with non-real-time device drivers.•Implementation details on an embedded platform considering software compatibility.</p> <p>This paper proposes a real-time (RT) control architecture based on Xenomai, an RT embedded Linux, to control a service robot along with non-real-time (NRT) robot operating system (ROS) packages. Most software, including device drivers and ROS, are developed to operate under the standard Linux kernel that does not provide RT guarantees. Standard Linux system calls in an RT context stimulates mode switching, resulting in non-deterministic responses and stability problems such as priority inversion and kernel panic. This paper overcomes such issues through a communication interface...},
author = {Delgado, Raimarius and You, Bum-Jae and Choi, Byoung Wook},
issn = {0164-1212},
journal = {The Journal of Systems & Software},
keywords = {Real-Time Control Architecture ; Robot Operating System ; Xenomai ; Cross-Domain Datagram Protocol ; Service Mobile Robots ; Real-Time Control Architecture ; Robot Operating System ; Xenomai ; Cross-Domain Datagram Protocol ; Service Mobile Robots ; Computer Science},
language = {eng},
pages = {8-19},
publisher = {Elsevier Inc},
title = {Real-time control architecture based on Xenomai using ROS packages for a service robot},
volume = {151},
year = {2019},
}

